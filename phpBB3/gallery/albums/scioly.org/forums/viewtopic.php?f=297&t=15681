	display: block;
	font-size: 0.8em !important;
	text-transform: uppercase;
	width: 100%;
}
.bbcode-hide pre {
	display: none;
	font-family: 'Avenir', Arial, sans-serif;
	white-space: pre-wrap;
}
@media print {
	.bbcode-hide pre {
		display: block !important;
	}
